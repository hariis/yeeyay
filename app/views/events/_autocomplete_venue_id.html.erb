<ul>
  <% @venues.each do |venue| %>
  <li><%= "<span style='font-weight:bold;'>" + venue.display_name + "</span>" + " <br/><br/> " %>
          <div class="informal"><%= "<span style='padding-left:5px;'>"  + venue.street_address  + "</span>"  + 
          "<span style='visibility:hidden;'>" + venue.id.to_s + "</span>" %> 
          </div></li>
  <% end %>
</ul>
