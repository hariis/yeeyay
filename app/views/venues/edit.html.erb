<% if !@venue.created_by?(current_user) %>
   <%= render :text => "Only the member that added this venue can edit. <br/> If you feel the information is incorrect, please contact us." %>
<% else %>    
  <div id="new-form-entry">
  <div id="heading">Edit <i><%= @venue.name %></i></div>
  <%= render :partial => 'venue_form' , :locals => {:venue => @venue} %>
  </div>

<% end %>